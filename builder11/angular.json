{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-common": {
      "projectType": "application",
      "root": "projects/angular-common",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "../proxy/public/angular11/angular-common",
            "index": "projects/index.html",
            "main": "projects/main.ts",
            "tsConfig": "projects/tsconfig.app.json",
            "aot": true,
            "customWebpackConfig": {
              "path": "projects/angular-common/extra-webpack.config.js",
              "libraryName": "angular-common",
              "libraryTarget": "system"
            }
          },
          "configurations": {
            "production": {
              "index": "",
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        }
      }
    },
